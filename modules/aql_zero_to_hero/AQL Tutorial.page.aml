        label: 'Total Orders'
        formula: @aql count(demo.fct_order_items.id);;
        calc_type: 'measure'
        data_type: 'number'
      }
      calculation f_1c4966c {
        label: 'Order Per Users'
        formula: @aql count(demo.fct_order_items.id) * 1.0 / count(demo.dim_users.id);;
