  block v15: VizBlock {
    label: 'Average Order Value over time'
    viz: CombinationChart {
      dataset: ds_ecommerce
      filter {
        field: ref('ecommerce.order_master', 'order_created_at')
        operator: 'matches'
        value: '2023'
      }
      x_axis: VizFieldFull {
        ref: ref('ecommerce.order_master', 'order_created_at')
        transformation: 'datetrunc month'
        format {
          type: 'date'
        }
      }
      y_axis {
        series {
          mark_type: 'line'
          field {
            ref: ref('ecommerce.order_master', 'aov')
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
          settings {
            color: '#A50E43'
          }
        }
      }
      settings {
        pop {
          field: ref('ecommerce.order_master', 'order_created_at')
          duration: 1
          granularity: 'year'
          show_growth_rate: true
        }
      }
    }
  }
  block v16: VizBlock {
    label: 'Sales Funnel'
    viz: ConversionFunnel {
      dataset: ds_ecommerce
      values: [
        VizFieldFull {
          ref: ref('ecommerce.dim_users', 'users_signed_up_count')
          format {
            type: 'number'
          }
        },
        VizFieldFull {
          ref: ref('ecommerce.user_facts_aggr', 'buyers_count')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: ref('ecommerce.user_facts_aggr', 'repeated_buyers_count')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      ]
      settings {
        circle_color: '#18A59E'
      }
    }
  }
