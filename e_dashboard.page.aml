  block v14: VizBlock {
    label: 'Orders by Country Name and Status'
    viz: ColumnChart {
      dataset: ds_ecommerce
      calculation f_7be4e9b {
        label: 'Status'
        formula: @aml  case(
  when: ecommerce.fct_orders.status == 'cancelled', then: 'Cancelled',
  when: ecommerce.fct_orders.status == 'delivered', then: 'Delivered',
  when: ecommerce.fct_orders.status == 'refunded', then: 'Refunded'
  , else: 'others'
);;
        calc_type: 'dimension'
        data_type: 'text'
      }
      x_axis: VizFieldFull {
        ref: ref('ecommerce.dim_countries', 'name')
        format {
          type: 'text'
        }
      }
      legend: VizFieldFull {
        ref: 'f_7be4e9b'
        format {
          type: 'text'
        }
      }
      y_axis {
        settings {
          stack_series_by: 'value'
        }
        series {
          field {
            label: 'orders'
            ref: ref('ecommerce.fct_orders', 'total_order')
            aggregation: 'custom'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      }
      settings {
        legend_label: 'top'
      }
    }
  }
