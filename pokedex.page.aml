  block page_css: TextBlock {
    content: @md <style>
      @import url('https://fonts.googleapis.com/css2?family=Pixelify+Sans:wght@400..700&display=swap');
      @import url('https://fonts.googleapis.com/css2?family=Patrick+Hand+SC&family=Playpen+Sans:wght@100..800&display=swap');      
      
      .h-canvas text, .h-canvas p, .h-canvas div {
