      }
      .dark-header {
        width: 100%; 
        padding: 0 50px;
        height: 100%; 
        background: rgba(0,0,0,0.5);
        margin: 0 auto;
        transform: skew(-15deg);
        
      }
      div:has(>.dark-header) {
        padding-left: 50px;
        padding-right: 50px;
      } 
      .white-card {
          width: 100%; 
          height: 100%; 
          background: #fff; 
          border-radius: 10px;
          background-clip: padding-box;
          border: 10px solid rgba(255, 255, 255, 0.2);
      }
      .header-text {
        color: #ffffff;
        font-weight: 700; 
        font-size:50px;
      }
      .single-value .html-value:has(.header-text-left) {
        margin-left: 0;
      }
      .pixel-corners {
        clip-path: polygon(
          0px calc(100% - 20px),
          4px calc(100% - 20px),
          4px calc(100% - 12px),
